{
  "name": "ronasit/laravel-helpers",
  "description": "Provided helpers function and some helper class.",
  "keywords": ["laravel", "helpers"],
  "license": "MIT",
  "authors": [
    {
      "name": "Ronas IT",
      "homepage": "https://github.com/RonasIT/laravel-helpers/graphs/contributors"
    }
  ],
  "require": {
    "php": ">=7.4",
    "laravel/framework": ">=5.5.0",
    "guzzlehttp/guzzle": ">=6.0",
    "maatwebsite/excel": "3.*",
    "tecnickcom/tcpdf": "~6.7.4",
    "doctrine/dbal": "^3.6",
    "ext-json": "*",
    "php-mock/php-mock-phpunit": "^2.9"
  },
  "require-dev": {
    "fakerphp/faker": "^1.23",
    "mockery/mockery": "1.*",
    "phpunit/phpunit": "9.*",
    "php-coveralls/php-coveralls": "^2.5",
    "orchestra/testbench": "^6.25",
    "mpyw/laravel-database-mock": "0.0.1-alpha4"
  },
  "autoload": {
    "psr-4": {
      "RonasIT\\Support\\Tests\\": "tests/",
      "RonasIT\\Support\\Tests\\Support\\": "tests/support/",
      "RonasIT\\Support\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "files": [
      "tests/TestCase.php"
    ]
  },
  "minimum-stability": "alpha",
  "extra": {
    "laravel": {
      "providers": [
        "RonasIT\\Support\\HelpersServiceProvider"
      ]
    }
  }
}
